{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "nashvilleccr/map",
	"version": "0.1.0",
	"title": "Map (NCCR)",
	"category": "widgets",
	"icon": "location-alt",
	"description": "An interactive map for events and groups",
	"example": {},
	"attributes": {
		"preload": {
			"type": "boolean",
			"default": true
		},
		"height": {
			"type": "string",
			"default": "400px"
		},
		"mapId": {
			"type": "string",
			"default": "DEMO_MAP_ID"
		},
		"eventPinColor": {
			"type": "string",
			"default": "#ea4335"
		},
		"eventPinBorderColor": {
			"type": "string",
			"default": "#b31412"
		},
		"groupPinColor": {
			"type": "string",
			"default": "#d0b310"
		},
		"groupPinBorderColor": {
			"type": "string",
			"default": "#88760f"
		}
	},
	"supports": {
		"html": false,
		"interactivity": true
	},
	"textdomain": "nccr",
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScriptModule": "file:./view.js",
	"viewStyle": "file:./view.css"
}